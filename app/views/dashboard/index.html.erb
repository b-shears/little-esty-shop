<h1> <%= @merchant.name %> </h1>
 <p>  <%= link_to "My Items Index", merchant_items_path(@merchant) %>  </p>
 <p>  <%= link_to "My Invoices Index", merchant_invoices_path(@merchant) %>  </p>

<h2>Top 5 Customers</h2>
  <% @top_five_customers.each do |customer| %>
    <p> <%= customer.first_name %> <%= customer.last_name %> |
      Number of Purchases: <%= customer.successful_transactions %></p>
  <% end %>
